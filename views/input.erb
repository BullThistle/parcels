<h1>Template</h1>

<form  action="/output">
  <div class="form-group">
    <label for="width">Enter the width</label>
    <input  name="width" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="length">Enter the length</label>
    <input  name="length" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="height">Enter the height</label>
    <input  name="height" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="weight">Enter the weight</label>
    <input  name="weight" class="form-control" type="text">
  </div>
  <button type="submit" class="btn">Add them</button>
</form>
